{
  "name": "zed-themes",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "packageManager": "pnpm@8.15.8",
  "scripts": {
    "build": "remix vite:build",
    "build:validator": "npx -f tsx ./dev/buildValidator.ts",
    "build:schema": "pnpm json2ts ./dev/themeFamily.json ./app/themeFamily.d.ts --unknownAny=false --style.singleQuote --additionalProperties=false",
    "dev": "remix vite:dev",
    "dev:wrangler": "wrangler pages dev ./build/client",
    "fmt": "prettier . --write",
    "test:lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "test:tsc": "wrangler types && tsc",
    "test:fmt": "prettier . --check",
    "test:e2e": "pnpm exec playwright test",
    "sync:themes": "npx tsx ./dev/syncZedThemes.ts",
    "sync:themes:local": "npx tsx ./dev/syncZedThemesLocal.ts",
    "migrate:shares": "npx tsx ./dev/migrateShares.ts",
    "ui": "pnpm dlx shadcn-ui@latest"
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@remix-run/cloudflare": "^2.10.2",
    "@remix-run/cloudflare-pages": "^2.10.2",
    "@remix-run/react": "^2.10.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.1.6",
    "immutability-helper": "^3.1.1",
    "isbot": "^5.1.12",
    "lucide-react": "^0.408.0",
    "nanoid": "^5.0.7",
    "react": "^18.3.1",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "react-file-drop": "^3.1.6",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240712.0",
    "@playwright/test": "^1.45.1",
    "@preact/signals-react-transform": "^0.3.2",
    "@remix-run/dev": "^2.10.2",
    "@remix-run/eslint-config": "^2.10.2",
    "@remix-run/node": "^2.10.2",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitejs/plugin-react": "^4.3.1",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.19",
    "cloudflare-workers-kv-viewer": "^1.0.3",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "json-schema-to-typescript": "^14.1.0",
    "miniflare": "^3.20240701.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^3.64.0",
    "zx": "^8.1.4"
  },
  "resolutions": {
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "@typescript-eslint/typescript-estree": "^7"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
